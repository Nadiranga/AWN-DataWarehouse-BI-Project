                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    CAST(SOH.SalesOrderNumber AS NVARCHAR(20)) AS SalesOrderNumber,
    SOD.SalesOrderDetailID,
    SOD.OrderQty,
    SOD.UnitPrice,
    SOD.UnitPriceDiscount,
    SOD.LineTotal,
    SOH.OrderDate,
    SOH.DueDate,
    SOH.ShipDate,
    SOH.TerritoryID,
    
    -- KEYS
    CAST(P.ProductNumber AS NVARCHAR(50)) AS ProductNumber,
    CAST(E.NationalIDNumber AS NVARCHAR(50)) AS SalesPersonNationalID,
    CAST(C.AccountNumber AS NVARCHAR(50)) AS ResellerID,
    SOD.SpecialOfferID,
    CAST(ISNULL(CR.ToCurrencyCode, 'USD') AS NCHAR(3)) AS CurrencyCode,

    -- NEW: CALCULATE THE LINE NUMBER (1, 2, 3...)
    CAST(ROW_NUMBER() OVER(PARTITION BY SOH.SalesOrderID ORDER BY SOD.SalesOrderDetailID) AS TINYINT) AS SalesOrderLineNumber

FROM Sales.SalesOrderHeader AS SOH
INNER JOIN Sales.SalesOrderDetail AS SOD
    ON SOH.SalesOrderID = SOD.SalesOrderID
INNER JOIN Production.Product AS P
    ON SOD.ProductID = P.ProductID
LEFT JOIN HumanResources.Employee AS E
    ON SOH.SalesPersonID = E.BusinessEntityID
INNER JOIN Sales.Customer AS C
    ON SOH.CustomerID = C.CustomerID
LEFT JOIN Sales.CurrencyRate AS CR
    ON SOH.CurrencyRateID = CR.CurrencyRateID
WHERE SOH.OnlineOrderFlag = 0;</property>
